  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable.builtin": {
      "foreground": "{{RED}}"
    },
    "module": {
      "foreground": "{{RED}}"
    },
    "namespace": {
      "foreground": "{{RED}}"
    },
    "variable.defaultLibrary": {
      "foreground": "{{RED}}"
    },
    "class.decorator": {
      "foreground": "{{BLUE}}"
    },
    "enumMember": {
      "foreground": "{{PURPLE}}"
    },
    "property": {
      "foreground": "{{PURPLE}}"
    },
    "builtinConstant": {
      "foreground": "{{ORANGE}}"
    },
    "enum": {
      "foreground": "{{YELLOW}}"
    },
    "selfParameter": {
      "foreground": "{{YELLOW}}"
    },
    "type": {
      "foreground": "{{YELLOW}}"
    },
    "typeParameter": {
      "foreground": "{{YELLOW}}"
    },
    "interface": {
      "foreground": "{{YELLOW}}"
    }
  },
  "tokenColors": [
    {
      "name": "Base: Keywords",
      "scope": "keyword,storage.type,storage.modifier,keyword.operator.new",
      "settings": {
        "foreground": "{{MAGENTA}}"
      }
    },
    {
      "name": "Base: Comment",
      "scope": "comment,string.quoted.docstring",
      "settings": {
        "foreground": "{{NEUTRAL_DARK}}",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Base: Constants",
      "scope": "constant",
      "settings": {
        "foreground": "{{ORANGE}}"
      }
    },
    {
      "name": "Base: Strings",
      "scope": "string",
      "settings": {
        "foreground": "{{GREEN}}"
      }
    },
    {
      "name": "Base: Types",
      "scope": "support.type.primitive",
      "settings": {
        "foreground": "{{YELLOW}}"
      }
    },
    {
      "name": "Base: Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "{{CYAN}}"
      }
    },
    {
      "name": "Base: Functions",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "{{BLUE}}"
      }
    },
    {
      "name": "Base: Decorator",
      "scope": "punctuation.definition.decorator,punctuation.decorator",
      "settings": {
        "foreground": "{{BLUE}}"
      }
    },
    {
      "name": "Base: Parameters",
      "scope": "variable.parameter,variable.other.readwrite.alias",
      "settings": {
        "foreground": "{{RED}}"
      }
    },
    {
      "name": "Base: Classes",
      "scope": "entity.name.type.class,variable.language.this",
      "settings": {
        "foreground": "{{YELLOW}}"
      }
    },
    {
      "name": "JSX: Common Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "{{RED}}"
      }
    },
    {
      "name": "JSX: Component",
      "scope": "support.class.component",
      "settings": {
        "foreground": "{{YELLOW}}"
      }
    },
    {
      "name": "JSX: Attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "{{PURPLE}}"
      }
    },
    {
      "name": "JSON: Keys",
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "{{PURPLE}}"
      }
    },
    {
      "name": "Markdown: Headers, Bold, Italics",
      "scope": "markup.heading,markup.bold,markup.italic",
      "settings": {
        "foreground": "{{ORANGE}}"
      }
    },
    {
      "name": "Markdown: Underline",
      "scope": "markup.underline",
      "settings": {
        "foreground": "{{BLUE}}"
      }
    },
    {
      "name": "Markdown: Details",
      "scope": "punctuation.definition.list.begin,fenced_code.block.language",
      "settings": {
        "foreground": "{{GREEN}}"
      }
    },
    {
      "name": "Markdown: Image Title",
      "scope": "string.other.link",
      "settings": {
        "foreground": "{{MAGENTA}}"
      }
    },
    {
      "name": "Shell: String Variables",
      "scope": "variable.other.normal.shell",
      "settings": {
        "foreground": "{{NEUTRAL}}"
      }
    }
  ]
}
